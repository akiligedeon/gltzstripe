    "dependency-cruiser": "13.1.1",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.12",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-require-form-method": "1.0.2",
    "eslint-plugin-testing-library": "5.11.0",
    "eslint-plugin-vitest": "0.2.8",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "next-test-api-route-handler": "3.1.8",
    "prettier": "3.0.1",
    "setup-polly-jest": "0.11.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "vite-tsconfig-paths": "4.2.0"
  },
  "engines": {
    "npm": ">=8.0.0 <10.0.0",
    "node": ">=18.0.0 <19.0.0",
    "pnpm": ">=8.0.0 <9.0.0"
  },
  "packageManager": "pnpm@8.6.0",
  "pnpm": {
    "patchedDependencies": {
      "@types/testing-library__jest-dom@5.14.9": "patches/@types__testing-library__jest-dom@5.14.9.patch"
    },
    "overrides": {
      "@urql/exchange-auth>@urql/core": "3.2.2",
      "@typescript-eslint/parser": "$@typescript-eslint/parser"
    }
  }
}
